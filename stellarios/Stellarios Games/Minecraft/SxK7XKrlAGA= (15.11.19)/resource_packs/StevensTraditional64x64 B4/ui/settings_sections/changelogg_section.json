{
  "namespace": "changelogg_section",

  "title_label": {
    "type": "label",
    "size": [ "100%", "default" ],
    "color": "$a_color_format",
    "shadow": true,
    "offset": [ 15, 0 ]
  },

  "link_label": {
    "type": "label",
    "size": [ "100%", "default" ],
    "color": [ 1.0, 1.0, 0.333 ],
    "offset": [ 15, 0 ]
  },

  "changelogg_button@settings_common.section_toggle_base": {
    "$toggle_view_binding_name": "changelogg_button_toggle",
    "$glyph_texture": "textures/ui/changelogg_image",
    "$glyph_color_texture": "textures/ui/changelogg_image",
    "$glyph_size": [ 16, 16 ],
    "$button_text": "options.changelogg",
    "$toggle_property_bag": {
      "#panel_title": "options.changeloggtitle"
    }
  },

  "changelogg_section": {
    "type": "stack_panel",
    "bindings": [
      {
        "binding_type": "view",
